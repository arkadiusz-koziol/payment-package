{
    "name": "skilleton/payment-package",
    "description": "A Laravel package that integrates PayU and Przelewy24 payment gateways.",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Skilleton\\PaymentPackage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skilleton\\PaymentPackage\\Providers\\PaymentServiceProvider"
            ],
            "aliases": {
                "PaymentPackage": "Skilleton\\PaymentPackage\\Facades\\PaymentPackage"
            }
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.9",
        "openpayu/openpayu": "^2.3"
    }
}
